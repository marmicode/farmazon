{
  "version": 1,
  "projects": {
    "farmicode": {
      "projectType": "application",
      "schematics": {},
      "root": "apps/farmicode",
      "sourceRoot": "apps/farmicode/src",
      "prefix": "fc",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/farmicode",
            "index": "apps/farmicode/src/index.html",
            "main": "apps/farmicode/src/main.ts",
            "polyfills": "apps/farmicode/src/polyfills.ts",
            "tsConfig": "apps/farmicode/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/farmicode/src/favicon.ico",
              "apps/farmicode/src/assets"
            ],
            "styles": ["apps/farmicode/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/farmicode/src/environments/environment.ts",
                  "with": "apps/farmicode/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "farmicode:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "farmicode:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "farmicode:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/farmicode/tsconfig.app.json",
              "apps/farmicode/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/farmicode/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/farmicode/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "farmicode-e2e": {
      "root": "apps/farmicode-e2e",
      "sourceRoot": "apps/farmicode-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/farmicode-e2e/cypress.json",
            "tsConfig": "apps/farmicode-e2e/tsconfig.e2e.json",
            "devServerTarget": "farmicode:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "farmicode:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/farmicode-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/farmicode-e2e/**/*"]
          }
        }
      }
    },
    "catalog-api": {
      "root": "apps/catalog-api",
      "sourceRoot": "apps/catalog-api/src",
      "projectType": "application",
      "prefix": "catalog-api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/catalog-api",
            "main": "apps/catalog-api/src/main.ts",
            "tsConfig": "apps/catalog-api/tsconfig.app.json",
            "assets": ["apps/catalog-api/src/assets"],
            "buildLibsFromSource": true
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/catalog-api/src/environments/environment.ts",
                  "with": "apps/catalog-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "catalog-api:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/catalog-api/tsconfig.app.json",
              "apps/catalog-api/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/catalog-api/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/catalog-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "animal-ui": {
      "projectType": "library",
      "root": "libs/animal-ui",
      "sourceRoot": "libs/animal-ui/src",
      "prefix": "farmazon",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/animal-ui/tsconfig.lib.json",
              "libs/animal-ui/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/animal-ui/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/animal-ui/jest.config.js",
            "tsConfig": "libs/animal-ui/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/animal-ui/src/test-setup.ts"
          }
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/angular",
            "port": 4400,
            "config": {
              "configFolder": "libs/animal-ui/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "options": {
            "uiFramework": "@storybook/angular",
            "outputPath": "dist/storybook/animal-ui",
            "config": {
              "configFolder": "libs/animal-ui/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      },
      "schematics": {}
    },
    "animal-core": {
      "projectType": "library",
      "root": "libs/animal-core",
      "sourceRoot": "libs/animal-core/src",
      "prefix": "farmazon",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/animal-core/tsconfig.lib.json",
              "libs/animal-core/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/animal-core/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/animal-core/jest.config.js",
            "tsConfig": "libs/animal-core/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/animal-core/src/test-setup.ts"
          }
        }
      },
      "schematics": {}
    },
    "animal-ui-e2e": {
      "root": "apps/animal-ui-e2e",
      "sourceRoot": "apps/animal-ui-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/animal-ui-e2e/cypress.json",
            "tsConfig": "apps/animal-ui-e2e/tsconfig.e2e.json",
            "devServerTarget": "animal-ui:storybook"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "animal-ui:storybook:ci"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/animal-ui-e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/animal-ui-e2e/**"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "farmicode"
}
